{
  "name": "sample-aws-sam",
  "version": "1.0.0",
  "description": "This project contains source code and supporting files for a serverless application that you can deploy with the SAM CLI. It includes the following files and folders.",
  "main": "app.js",
  "scripts": {
    "build": "npm run webpack && sam build",
    "build:all": "npm run webpack && find dist/* -name \"*\" -type d -exec cp package.json {} \\; && sam build",
    "webpack": "NODE_ENV=development webpack",
    "watch": "npm run webpack --watch"
  },
  "repository": {},
  "author": "dhythm",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.47",
    "@types/glob": "^7.1.1",
    "@types/node": "^13.11.0",
    "@types/webpack": "^4.41.10",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "awesome-typescript-loader": "^5.2.1",
    "aws-sdk": "^2.653.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "glob": "^7.1.6",
    "prettier": "^2.0.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
